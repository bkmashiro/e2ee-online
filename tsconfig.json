{
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.ts", "src/**/*.tsx", "src/**/*.d.ts"],
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    },
    "baseUrl": ".",
    "moduleResolution": "Node",
    "allowUnusedLabels": true,
    "strictPropertyInitialization": false,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "module": "esnext"
  }
}
